
{
  "manifest_version": 3,
  "name": "NanoSum",
  "version": "1.0",
  "description": "Get instant, clear summaries of any web page using Gemini Nano.",
  "permissions": [
    "activeTab",
    "scripting",
    "ai" 
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  }
}